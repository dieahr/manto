<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.uag.iidis.scec.modelo.Expedicion"
           table="Expediciones"
           dynamic-update="false"
           dynamic-insert="false">
        
        <id name="id"
            column="id"
            type="java.lang.Long"
            unsaved-value="null" >
            <generator class="increment">
            </generator>
        </id>
        
        <property name="nombre"
                  column="Nombre"
                  type="java.lang.String"
                  not-null="true"
                  unique="true" />
        
        <property name="descripcion"
                  column="Descripcion"
                  type="java.lang.String" />
        
        <property name="cantidad"
                  column="Cantidad"
                  type="java.lang.String" />
        
        <property name="lugarencuentro"
                  column="LugarEncuentro"
                  type="java.lang.String" />
        
        <property name="estado"
                  column="Estado"
                  type="java.lang.String" />
        
        <property name="pais"
                  column="Pais"
                  type="java.lang.String" />
        
        <property name="direccion"
                  column="Direccion"
                  type="java.lang.String" />
        
        <property name="fecha"
                  column="Fecha"
                  type="java.lang.String" />
        
        <property name="hora"
                  column="Hora"
                  type="java.lang.String" />
        
    </class>
</hibernate-mapping>
